{
  "cli": {
    "appVersionSource": "remote",
    "version": ">=2.0.0"
  },
  "build": {
    "development": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "ENVIRONMENT": "development"
      }
    },
    "production": {
      "env": {
        "ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "development": {
      "android": {
        "releaseStatus": "draft",
        "track": "internal"
      }
    },
    "production": {}
  }
}
